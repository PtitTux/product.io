{
  "compilerOptions": {
    "target": "ESNext",
    "baseUrl": "./src",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "noImplicitAny": true,
    "outDir": "./dist",
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "esModuleInterop": true
  },
  "include": [
    "./src/**/*"
  ]
}
